# This is a basic workflow to help you get started with Actions

name: ChallengeOne

on: push

jobs:
  ubuntu:
    runs-on: ubuntu-latest
    steps:
      - run: date
 windows
    runs-on: windows-latest
    steps
      - run: date
  macos:
    runs-on: macos-latest
    steps:
      - run: date
  job4:
    runs-on: macos-latest
    needs : [ubuntu, windows, macos]
    steps:
      - run: date
